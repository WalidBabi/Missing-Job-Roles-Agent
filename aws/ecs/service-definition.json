  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "REPLACE_WITH_PRIVATE_SUBNET_1",
        "REPLACE_WITH_PRIVATE_SUBNET_2"
